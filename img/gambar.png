<link href="//storage.googleapis.com/wsafari/css/bootstrap.min.css" rel="stylesheet">
<link href="//storage.googleapis.com/wsafari/css/more.css" rel="stylesheet">
<script src="//storage.googleapis.com/wsafari/js/jquery.min.js"></script>
<script src="//storage.googleapis.com/wsafari/js/lazy.min.js"></script>
<script>
		$(document).ready(function() {
		    var bLazy = new Blazy({
	            offset: 100,
	            success: function(element) {
	                setTimeout(function() {
	                    var parent = element.parentNode;
	                    parent.className = parent.className.replace(/\bloading\b/, '');
	                }, 200);
	            },
	            error: function(ele, msg) {
	                if (msg === 'missing') {
	                    // Data-src is missing
	                } else if (msg === 'invalid') {
	                    $(ele).attr('width','728').attr('height','485').attr('src','/img/404.png').css( "height", "auto" ).css( "opacity", "1" );
	                }
	            }
	        });
		});
	</script>
<script src="//storage.googleapis.com/wsafari/js/bootstrap.min.js" async></script>
<script src="//storage.googleapis.com/wsafari/js/scripts.js" async></script>

